{
  "version": 2,
  "name": "marketpace-web-app",
  "alias": ["MarketPace.shop", "www.MarketPace.shop"],
  "builds": [
    {
      "src": "pitch-page.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/pitch-page.js"
    },
    {
      "src": "/enhanced-community-feed.html",
      "dest": "/enhanced-community-feed.html"
    },
    {
      "src": "/driver-application.html",
      "dest": "/driver-application.html"
    },
    {
      "src": "/admin-login.html",
      "dest": "/admin-login.html"
    },
    {
      "src": "/admin-dashboard.html",
      "dest": "/admin-dashboard.html"
    },
    {
      "src": "/sponsorship",
      "dest": "/pitch-page.js"
    },
    {
      "src": "/demo-login.html",
      "dest": "/demo-login.html"
    },
    {
      "src": "/enhanced-signup.html",
      "dest": "/enhanced-signup.html"
    },
    {
      "src": "/password-reset.html",
      "dest": "/password-reset.html"
    },
    {
      "src": "/profile.html",
      "dest": "/profile.html"
    },
    {
      "src": "/community.html",
      "dest": "/community.html"
    },
    {
      "src": "/driver-dash-demo.html",
      "dest": "/driver-dash-demo.html"
    },
    {
      "src": "/driver-success.html",
      "dest": "/driver-success.html"
    },
    {
      "src": "/(.*)",
      "dest": "/pitch-page.js"
    }
  ],
  "env": {
    "STRIPE_SECRET_KEY": "@stripe_secret_key",
    "VITE_STRIPE_PUBLIC_KEY": "@vite_stripe_public_key",
    "TWILIO_ACCOUNT_SID": "@twilio_account_sid",
    "TWILIO_AUTH_TOKEN": "@twilio_auth_token",
    "TWILIO_PHONE_NUMBER": "@twilio_phone_number",
    "DATABASE_URL": "@database_url",
    "PGHOST": "@pghost",
    "PGPORT": "@pgport",
    "PGUSER": "@pguser",
    "PGPASSWORD": "@pgpassword",
    "PGDATABASE": "@pgdatabase"
  },
  "functions": {
    "pitch-page.js": {
      "maxDuration": 10
    }
  }
}